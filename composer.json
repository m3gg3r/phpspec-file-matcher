{
  "name": "ecomdev/phpspec-file-matcher",
  "description": "PHPSpec extension that adds file matcher",
  "keywords": [
    "phpspec",
    "extension",
    "matcher"
  ],
  "type": "library",
  "license": "MIT",
  "require": {
     "php": ">= 5.4"
  },
  "require-dev": {
    "henrikbjorn/phpspec-code-coverage": "3.0.x-dev",
    "phpspec/nyan-formatters": "^2.0",
    "satooshi/php-coveralls": "^1.0",
    "mikey179/vfsStream": "^1.6",
    "phpspec/phpspec": "~3.0",
    "coduo/phpspec-data-provider-extension": "dev-master",
    "squizlabs/php_codesniffer": "^2.6"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "bin-dir": "bin"
  },
  "autoload": {
    "psr-4": {
      "EcomDev\\PHPSpec\\FileMatcher\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "spec\\EcomDev\\PHPSpec\\FileMatcher\\": "spec/"
    }
  },
  "authors": [
    {
      "name": "Ivan Chepurnyi",
      "email": "ivan@ecomdev.org"
    }
  ],
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/m3gg3r/phpspec-data-provider-extension.git"
    }
  ]
}
